set terminal png size 1280,720
set output "BtLatency.png"
set title "Bluetooth Arduino to PC Latency"
set xlabel "Data Packet"
set ylabel "Latency [ms]"
set xrange [0:65535]
set yrange [0:50]
set grid
set key box

plot "BtLatency.data" u 1:2 every ::0::65535 w points lt rgb "#000000" t "Latency", \
                   "" u 1:2 every 128::0::65535 w l lt rgb "#FF0000" t "Approx. Latency Development"

